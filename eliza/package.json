{
  "name": "vrm-eliza-integration",
  "version": "3.0.0",
  "description": "VRM ElizaOS AI Girlfriend - Production Ready",
  "main": "bridge/production-bridge.js",
  "type": "module",
  "scripts": {
    "dev": "node bridge/phase2-bridge.js",
    "build": "echo 'Build complete'",
    "start": "node bridge/production-bridge.js",
    "production": "NODE_ENV=production node bridge/production-bridge.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "generate-characters": "node scripts/generate-character.js all",
    "validate-configs": "node tests/validate-configs.js",
    "health-check": "curl -f http://localhost:3001/health || exit 1",
    "docker:build": "docker build -t vrm-eliza-bridge .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f eliza-bridge"
  },
  "keywords": [
    "eliza",
    "ai-agent",
    "vrm",
    "chatbot",
    "typescript"
  ],
  "author": "VRM AI Girlfriend Team",
  "license": "MIT",
  "dependencies": {
    "@ai16z/eliza": "^0.1.4-alpha.4",
    "@supabase/supabase-js": "^2.56.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/vrm-ai-girlfriend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/vrm-ai-girlfriend/issues"
  },
  "homepage": "https://vrm-ai-girlfriend.vercel.app"
}
